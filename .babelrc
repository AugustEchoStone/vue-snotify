{
  "presets": [
    [
      "env",
    {
      "modules": false,
      "targets": {
        "browsers": ["last 2 versions", "safari >= 7", "not ie <= 8"]
      }
    }
    ],
    "stage-2"
  ],
  "plugins": [
    "syntax-class-properties",
    "transform-class-properties",
    "external-helpers",
    "transform-object-rest-spread"
  ],
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [
        "istanbul"
      ]
    }
  }
}
